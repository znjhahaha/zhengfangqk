{
  "name": "tyust-course-selector",
  "version": "1.0.0",
  "description": "太原科技大学选课工具 - Next.js现代化界面",
  "private": true,
  "scripts": {
    "dev": "next dev -H 127.0.0.1",
    "build": "node scripts/prepare-static-export.js && next build && node scripts/restore-dynamic-routes.js",
    "build:analyze": "ANALYZE=true next build",
    "build:apk": "BUILD_APK=true npm run build && npx cap sync android",
    "build:apk:full": "npm run build && npx cap sync android && npx cap open android",
    "start": "next start",
    "lint": "next lint",
    "perf": "next build && next start",
    "clean": "rm -rf .next out",
    "cap:sync": "npx cap sync",
    "cap:open": "npx cap open android",
    "cap:add": "npx cap add android",
    "cap:init": "npx cap init \"正方教务工具\" \"com.tyust.course.selector\" --web-dir=out"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-progress": "1.0.3",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-switch": "1.0.3",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-toast": "1.1.5",
    "@types/node": "20.0.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/react-window": "^1.8.8",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "10.4.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "cos-nodejs-sdk-v5": "^2.15.4",
    "framer-motion": "10.16.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "0.292.0",
    "next": "14.0.0",
    "postcss": "8.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.2.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.3.0",
    "typescript": "^5.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@types/html2canvas": "^0.5.35",
    "eslint": "8.0.0",
    "eslint-config-next": "14.0.0",
    "null-loader": "^4.0.1"
  }
}
